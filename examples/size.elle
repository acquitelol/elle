external fn printf(string formatter, ...);

fn other(int *buf) {
    printf("(fn other)\n\t#size(buf) = %d\n", #size(buf));
}

pub fn main() {
    int buf[100];
    buf[0] = 123;

    printf(
        "(fn main)\n\t#size(buf) = %d\n\t#arrlen(buf) = %d\n",
        #size(buf),
        #arrlen(buf)
    );

    other(buf);
    return 0;
}
