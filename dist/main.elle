fn input(String message) {
    Long stdin = fdopen(0, "r");
    Char buf[1024];

    printf(message);
    fgets(buf, 1024, stdin);

    Long len = strlen(buf);
    Long index = len - 1;
    Long newline = buf + index;

    newline <- Byte 0;
    ret buf;
}

pub fn main() {
    String res = input("Input > ");
    printf!("Did you say %s? That's so true bestie!!\n", res);
}